{
  "cSpell.words": [
    "codespace",
    "IPFS",
    "multibase",
    "Roadmap",
    "tendermint",
    "webauthn"
  ],
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true,
    "**/node_modules": true,
    "types/common": true,
    "types/crypto/vc.pb.go": true,
    "types/crypto/coin.pb.go": true,
    "types/crypto/credential.pb.go": true,
    "types/crypto/pubkey.pb.go": true,
    "types/highway": false,
    "x/identity/types/genesis.pb.go": true,
    "x/identity/types/query.pb.go": true,
    "x/identity/types/query.pb.gw.go": true,
    "x/identity/types/tx.pb.go": true,
    "x/identity/types/did.pb.go": true
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "hide-files.files": [
    "types/common/info.pb.go",
    "types/crypto/vc.pb.go",
    "types/crypto/coin.pb.go",
    "types/crypto/credential.pb.go",
    "types/crypto/pubkey.pb.go",
    "types/highway",
    "x/identity/types/genesis.pb.go",
    "x/identity/types/query.pb.go",
    "x/identity/types/query.pb.gw.go",
    "x/identity/types/tx.pb.go",
    "x/identity/types/did.pb.go"
  ],
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "explorerExclude.backup": {},
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "default",
  "go.lintTool": "golint"
}

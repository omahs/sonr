{
  "cSpell.words": [
    "codespace",
    "IPFS",
    "multibase",
    "Roadmap",
    "tendermint",
    "webauthn"
  ],
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/node_modules": true,
    "types/common": true,
    "frontend/apps/web/tailwind.config.js": true,
    "types/common/info.pb.go": true,
    "types/crypto/vc.pb.go": true,
    "types/crypto/coin.pb.go": true,
    "types/crypto/credential.pb.go": true,
    "types/crypto/pubkey.pb.go": true,
    "x/identity/types/query.pb.go": true,
    "x/identity/types/query.pb.gw.go": true,
    "x/identity/types/tx.pb.go": true,
    "x/identity/types/did.pb.go": true,
    "frontend/.npmrc": true,
    "frontend/.gitignore": true,
    ".gitignore": true,
    "LICENSE": true,
    "docs/api-reference": true,
    "testutil": true,
    "scripts": true,
    "proto/buf.lock": true,
    "go.sum": true,
    "core.code-workspace": true,
    "package-lock.json": true,
    "turbo.json": false,
    ".eslintrc.js": true,
    "buf.work.yaml": true,
    "Dockerfile": true,
    "Makefile": true,
    "package.json": false
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "hide-files.files": [
    "types/common/info.pb.go",
    "types/crypto/vc.pb.go",
    "types/crypto/coin.pb.go",
    "types/crypto/credential.pb.go",
    "types/crypto/pubkey.pb.go",
    "x/identity/types/query.pb.go",
    "x/identity/types/query.pb.gw.go",
    "x/identity/types/tx.pb.go",
    "x/identity/types/did.pb.go",
    "frontend/.npmrc",
    "frontend/.gitignore",
    ".gitignore",
    "LICENSE",
    "docs/api-reference",
    "testutil",
    "scripts",
    "proto/buf.lock",
    "go.sum"
  ],
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "explorerExclude.backup": {},
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "default",
  "go.lintTool": "golint",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#1f6fd0",
    "activityBar.background": "#1f6fd0",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ee90bb",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1f6fd0",
    "statusBar.background": "#1857a4",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#1f6fd0",
    "statusBarItem.remoteBackground": "#1857a4",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#1857a4",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#1857a499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#1857a4"
}

{
  "gitlens.views.formats.commits.label": "${❰ tips ❱➤  }${message}",
  "gitlens.views.formats.commits.description": "${author, }${agoOrDate}${tips}",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.views.commitDetails.files.compact": false,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#65cbe9",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#37474F",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "!**/.git/hooks": true,
    ".dart_tool": true,
    "**/.classpath": true,
    "**/.next": true,
    "**/.turbo": true,
    "./**/.vscode": true,
    "**/.factorypath": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.packages": true,
    "**/.project": true,
    "**/.settings": true,
    "**/largeFolder": true,
    "**/pubspec.lock": true,
    "**pubspec.lock": true,
    "app.iml": true,
    "Contents.json": true,
    "**/.trunk/*out/": true,
    "frontend/**/client/lib": true,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*plugins/": true,
    "**/node_modules": true,
    "types/common": true,
    "types/common/info.pb.go": true,
    "types/crypto/vc.pb.go": true,
    "types/crypto/coin.pb.go": true,
    "types/crypto/credential.pb.go": true,
    "types/crypto/pubkey.pb.go": true,
    "x/identity/types/query.pb.go": true,
    "x/identity/types/query.pb.gw.go": true,
    "x/identity/types/tx.pb.go": true,
    "x/identity/types/did.pb.go": true,
    "frontend/.npmrc": true,
    "frontend/.gitignore": true,
    "LICENSE": true,
    "proto/buf.lock": true,
    "go.sum": true,
    "core.code-workspace": true,
    "package-lock.json": true,
    "turbo.json": false,
    ".eslintrc.js": true,
    "buf.work.yaml": true,
    "Dockerfile": true,
    "Makefile": true,
    "package.json": false,
    ".devcontainer": true,
    "testutil": true
  },
  "svg.preview.background": "custom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[go]": {
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnType": true
  },
  "go.autocompleteUnimportedPackages": true,
  "go.formatTool": "default",
  "go.lintTool": "golint",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "explorer.excludeGitIgnore": true
}
